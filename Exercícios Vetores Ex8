#include <stdio.h>

int main() {
   
    int vetor[6];
    
    printf("Informe os 6 elementos do vetor: \n");
    for (int i = 0; i < 6; i++) {
        scanf("%d", &vetor[i]);
    }
    printf("Vetor na ordem inversa: ");
    for (int i = 5; i >= 0; i--) {
        printf("%d ", vetor[i]);
    }
    printf("\n");
}
