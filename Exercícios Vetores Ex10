#include <stdio.h>

int main() {
    float notas[15];
    float soma = 0;

    printf("Informe as notas dos 15 alunos: \n");
    for (int i = 0; i < 15; i++) {
        scanf("%f", &notas[i]);
        soma += notas[i];
    }
    float media = soma / 15.0;
    printf("A media geral das notas e: %.2f\n", media);
}
