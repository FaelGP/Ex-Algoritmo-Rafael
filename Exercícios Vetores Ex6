#include <stdio.h>

int main() {
   
    int vetor[10];
    
   
    printf("Informe os 10 elementos do vetor: \n");
    for (int i = 0; i < 10; i++) {
        scanf("%d", &vetor[i]);
    }
    
    int maior = vetor[0];
    for (int i = 1; i < 10; i++) {
        if (vetor[i] > maior) {
            maior = vetor[i];
        }
    }
    
    int menor = vetor[0];
    for (int i = 1; i < 10; i++) {
        if (vetor[i] < menor) {
            menor = vetor[i];
        }
    }
 
    printf("O maior elemento do vetor e: %d\n", maior);
    printf("O menor elemento do vetor e: %d\n", menor);
}
