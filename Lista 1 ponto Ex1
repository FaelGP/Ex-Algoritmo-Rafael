#include <stdio.h>

int main() {
    int A, B, C, D, diferenca;
    
    printf("Informe o valor de A: ");
    scanf("%d", &A);
    
    printf("Informe o valor de B: ");
    scanf("%d", &B);
    
    printf("Informe o valor de C: ");
    scanf("%d", &C);
    
    printf("Informe o valor de D: ");
    scanf("%d", &D);
    
    diferenca = A * B - C * D;
    
    printf("DIFERENCA = (%d * %d - %d * %d)\n", A, B, C, D);
    printf("DIFERENCA = %d\n", diferenca);
    
    return 0;
}
