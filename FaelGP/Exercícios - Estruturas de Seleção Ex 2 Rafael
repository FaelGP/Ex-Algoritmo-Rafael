#include <stdio.h>

main(){

    int estado;
    float valor, imposto;

    printf("Informe o valor do produto: ");
    scanf("%f", &valor);

    printf("Informe o estado \n 1-DF \n 2-RJ \n 3-SP \n 4-BH \n");
    scanf("%d",&estado);

    switch (estado){
    case 1:
    imposto = valor + (valor0.09);
    printf("O valor do produto com imposto para DF e de :%.2f",imposto);
        break;

        case 2:
        imposto = valor + (valor0.15);
        printf("O valor do produto com imposto para RJ e de :%.2f",imposto);
        break;

        case 3:
         imposto = valor + (valor0.11);
         printf("O valor do produto com imposto para SP e de :%.2f",imposto);
        break;

        case 4:
        imposto = valor + (valor0.09);
        printf("O valor do produto com imposto para BH e de :%.2f",imposto);
        break;

    default:
    printf("Erro Digite o numero de um estado existente.");
        break;
    }
